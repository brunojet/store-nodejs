{
  "name": "store_nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prisma:generate": "prisma generate",
    "build": "npm run prisma:generate && tsc -b",
    "postinstall": "npm run prisma:generate",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/__tests__/**"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "prisma": "^6.13.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.4.0"
  }
