{
  "name": "store_nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prisma:generate": "prisma generate",
    "build": "npm run prisma:generate && tsc -b",
    "postinstall": "npm run prisma:generate",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint src/**/*.{js,ts} --max-warnings=0",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "clang": "node scripts/clang-all.js",
    "check-all": "npm run typecheck && npm run lint && npm run clang",
    "prepare": "npm run check-all"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/__tests__/**"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "express": "^5.1.0",
    "prisma": "^6.13.0",
    "typescript": "^5.8.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "clang-format": "^1.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  }
}
