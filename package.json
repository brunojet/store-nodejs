{
  "name": "store_nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prisma:generate": "prisma generate",
    "build": "npm run prisma:generate && tsc -b",
    "postinstall": "npm run prisma:generate",
    "single": "npx cross-env WORKER_MULTIPLIER=1 MAX_WORKERS=1 tsx src/main-cluster.ts",
    "cluster": "npx tsx src/main-cluster.ts",
    "dev": "npm run single",
    "prod": "node dist/main-cluster.js",
    "start": "npm run prod",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint src/**/*.{js,ts} --max-warnings=0",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "clang": "node scripts/clang-all.js",
    "check-all": "npm run typecheck && npm run lint && npm run clang",
    "prepare": "npm run check-all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "check-disk-space": "^3.4.0",
    "express": "^5.1.0",
    "prisma": "^6.13.0",
    "typescript": "^5.8.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "clang-format": "^1.8.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3"
  }
}
